{
  "name": "dataprep-wrangle-language",
  "displayName": "Cloud Dataprep Wrangle",
  "description": "Language support for Cloud Dataprep Wrangle by TrifactaÂ®",
  "version": "0.0.1",
  "publisher": "chemistechlabs",
  "repository": {
    "type": "git",
    "url": "https://github.com/chemistechlabs/dataprep-wrangle-language"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#222222",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "wrangle",
        "aliases": [
          "Wrangle",
          "wrangle"
        ],
        "extensions": [
          ".wrangle"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wrangle",
        "scopeName": "source.wrangle",
        "path": "./syntaxes/wrangle.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "wrangle",
        "path": "./snippets/wrangle.code-snippets.json"
      }
    ]
  }
}